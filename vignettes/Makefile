
all: eglen2015.pdf


eglen2015.pdf: eglen2015.Rnw eglen2015.bib
	Rscript -e 'knitr::knit2pdf("eglen2015.Rnw")'


## Remove all of the PDFs that were autogenerated
## except for layers_csn3.pdf
PDF1 := $(wildcard *.pdf)
PDF     := $(filter-out layers_csn3.pdf, $(PDF1))

test:
	echo $(PDF)

.PHONY: clean

clean:
	rm -f $(PDF)
	latexmk -c
	rm -f *.fff *.ttt figchunks.csv eglen2015.tex


